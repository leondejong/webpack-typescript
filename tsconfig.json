{
    "extends": "@tsconfig/recommended/tsconfig.json",
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended",
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strictPropertyInitialization": false
    }
}
